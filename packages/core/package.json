{
  "name": "@arethetypeswrong/core",
  "version": "0.15.1",
  "description": "The API behind arethetypeswrong.github.io",
  "author": "Andrew Branch",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/arethetypeswrong/arethetypeswrong.github.io.git",
    "directory": "packages/core"
  },
  "files": [
    "LICENSE",
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "node --experimental-transform-types --conditions=@arethetypeswrong/source --test 'test/**/*.test.ts'",
    "test-js": "node --experimental-transform-types --test 'test/**/*.test.ts'",
    "snapshot": "node scripts/createSnapshotFixture.js",
    "prepack": "pnpm tsc"
  },
  "type": "module",
  "imports": {
    "#internal/*": {
      "@arethetypeswrong/source": "./src/internal/*.ts",
      "default": "./dist/internal/*.js"
    }
  },
  "exports": {
    ".": {
      "@arethetypeswrong/source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "@arethetypeswrong/source": "./src/types.ts",
      "default": "./dist/types.js"
    },
    "./utils": {
      "@arethetypeswrong/source": "./src/utils.ts",
      "default": "./dist/utils.js"
    },
    "./problems": {
      "@arethetypeswrong/source": "./src/problems.ts",
      "default": "./dist/problems.js"
    },
    "./versions": {
      "node": "./dist/versions.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@andrewbranch/untar.js": "^1.0.3",
    "fflate": "^0.8.2",
    "semver": "^7.5.4",
    "typescript": "5.6.1-rc",
    "validate-npm-package-name": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "@types/semver": "^7.5.0",
    "@types/validate-npm-package-name": "^4.0.0",
    "ts-expose-internals": "5.6.1-rc"
  },
  "engines": {
    "node": ">=18"
  }
}
